{
  "name": "@abraxas/voting-basis",
  "version": "0.0.0-development",
  "scripts": {
    "prepare": "husky install",
    "start": "ng serve",
    "start:sta": "ng serve -c=local-sta",
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "test:integration": "testcafe chrome ./e2e/**/*.ts --hostname=localhost --port=4200",
    "test:integration:debug": "testcafe chrome ./e2e/**/*.ts --hostname=localhost --port=4200 --debug-on-fail",
    "test:integration:ci": "testcafe chrome:headless ./e2e/**/*.ts --app=\"npm run start\" --hostname=localhost --port=4200 --app-init-delay=60000 --quarantine-mode",
    "lint": "ng lint && stylelint \"./src/**/*.scss\" && prettier --check ./src/**/*.{ts,scss}",
    "lint:ng": "ng lint",
    "lint:license": "eslint --fix .",
    "fix": "prettier --write ./src/**/*.{ts,scss} && ng lint --fix"
  },
  "private": true,
  "dependencies": {
    "@abraxas/base-components": "17.2.0",
    "@abraxas/voting-basis-service-proto": "1.93.0",
    "@abraxas/voting-lib": "4.0.1",
    "@angular/animations": "17.2.0",
    "@angular/cdk": "17.2.0",
    "@angular/cdk-experimental": "17.2.0",
    "@angular/common": "17.2.0",
    "@angular/compiler": "17.2.0",
    "@angular/core": "17.2.0",
    "@angular/forms": "17.2.0",
    "@angular/material": "17.2.0",
    "@angular/material-moment-adapter": "17.2.0",
    "@angular/platform-browser": "17.2.0",
    "@angular/platform-browser-dynamic": "17.2.0",
    "@angular/router": "17.2.0",
    "@ngx-translate/core": "^14.0.0",
    "google-protobuf": "3.21.0",
    "grpc-web": "1.3.1",
    "moment": "2.29.4",
    "ngx-currency": "2.5.3",
    "ngx-drag-drop": "17.0.0",
    "ngx-mask": "13.1.15",
    "ngx-moment": "6.0.2",
    "otplib": "12.0.1",
    "rxjs": "7.8.1",
    "slugify": "1.6.5",
    "tslib": "2.4.0",
    "zone.js": "0.14.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "17.2.0",
    "@angular-eslint/builder": "17.2.0",
    "@angular-eslint/eslint-plugin": "17.2.0",
    "@angular-eslint/eslint-plugin-template": "17.2.0",
    "@angular-eslint/schematics": "17.2.0",
    "@angular-eslint/template-parser": "17.2.0",
    "@angular/cli": "17.2.0",
    "@angular/compiler-cli": "17.2.0",
    "@angular/language-service": "17.2.0",
    "@angularclass/hmr": "3.0.0",
    "@types/google-protobuf": "3.15.6",
    "@types/jest": "28.1.6",
    "@types/jsdom": "20.0.0",
    "@types/node": "18.6.5",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "eslint": "^8.57.0",
    "eslint-plugin-license-header": "0.6.0",
    "husky": "8.0.1",
    "jest": "29.7.0",
    "jest-preset-angular": "13.1.4",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "lint-staged": "13.0.3",
    "ng-lint-staged": "12.0.4",
    "prettier": "2.7.1",
    "prettier-stylelint": "0.4.2",
    "stylelint": "14.9.1",
    "stylelint-config-standard-scss": "5.0.0",
